      <% users.each do |user| %>
      <tbody>
      <tr>
        <td><%= attachment_image_tag user,:profile_image, :fill,30,30, format: 'jpeg',fallback: "no_image.jpg",size:'60x60' %></td>
        <td><%= user.name %></td>
        <td>
          <%= render 'relationships/follow_button', user:user %>
        </td>
        <td><%= link_to 'Show', user_path(user.id) %></td>
        <% end %>
      </tr>
    </tbody>